  "scripts": {
    "build": "npx tsc --incremental",
    "setup": "yarn install && yarn link magic && yarn link model && yarn link storage && yarn build",
    "start-ts": "tsc -w",
    "start-node": "PORT=3000 nodemon dist/index.js",
    "start": "concurrently -k -p \"[{name}]\" -n \"tsc,node\" \"npm run start-ts\" \"npm run start-node\"",
    "test": "jest --forceExit --verbose",
    "migrate": "npm run build; node dist/db/migrate.js",
    "rollback": "npm run build; node dist/db/rollback.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.8",
    "@types/node-fetch": "^2.5.12",
    "concurrently": "^7.0.0",
    "jest": "^27.4.7",
    "node-fetch": "2.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1"
  },
  "dependencies": {
    "@types/jest": "^27.4.0",
    "@types/pg": "^8.6.3",
    "express": "^4.17.2",
    "fp-ts": "^2.11.6",
    "fp-ts-contrib": "^0.1.26",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "monocle-ts": "^2.3.12",
    "newtype-ts": "^0.3.5",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "typescript": "^4.5.4"
  }
